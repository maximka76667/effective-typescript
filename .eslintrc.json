{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "@typescript-eslint/no-unused-vars": "off",
        "indent": [
            "error",
            2
        ],
        "no-redeclare": "off",
        "quotes": [
            "error",
            "double"
        ],
        "linebreak-style": [
            "error",
            "windows"
        ],
        "max-len": [
            "error",
            {
                "code": 90,
                "comments": 100
            }
        ],
        "no-unused-vars": "off",
        "no-unused-expressions": "off",
        "no-console": "off",
        "no-shadow": "off",
        "no-restricted-syntax": "off",
        "no-underscore-dangle": "off",
        "no-plusplus": "off",
        "max-classes-per-file": "off",
        "no-new-wrappers": "off",
        "indent": [
            "error",
            2,
            {
                "SwitchCase": 1
            }
        ]
    },
    "overrides": [
        {
            "files": [
                "*.ts"
            ],
            "rules": {
                "no-unused-vars": "off",
                "no-console": "off"
            }
        }
    ]
}